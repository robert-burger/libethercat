#ifndef LIBETHERCAT_SETTINGS_H
#define LIBETHERCAT_SETTINGS_H

/* Maximum number of coe-emergencies supported. */
#undef LIBETHERCAT_MAX_COE_EMERGENCIES

/* Maximum number of data supported. */
#undef LIBETHERCAT_MAX_DATA

/* Maximum number of datagrams supported. */
#undef LIBETHERCAT_MAX_DATAGRAMS

/* Maximum number of ds402-subdevs supported. */
#undef LIBETHERCAT_MAX_DS402_SUBDEVS

/* Maximum number of eeprom-cat-dc supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_DC

/* Maximum number of eeprom-cat-fmmu supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_FMMU

/* Maximum number of eeprom-cat-pdo supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_PDO

/* Maximum number of eeprom-cat-pdo-entries supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_PDO_ENTRIES

/* Maximum number of eeprom-cat-sm supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_SM

/* Maximum number of eeprom-cat-strings supported. */
#undef LIBETHERCAT_MAX_EEPROM_CAT_STRINGS

/* Maximum number of groups supported. */
#undef LIBETHERCAT_MAX_GROUPS

/* Maximum number of init-cmd-data supported. */
#undef LIBETHERCAT_MAX_INIT_CMD_DATA

/* Maximum number of mbx-entries supported. */
#undef LIBETHERCAT_MAX_MBX_ENTRIES

/* Maximum number of pdlen supported. */
#undef LIBETHERCAT_MAX_PDLEN

/* Maximum number of slaves supported. */
#undef LIBETHERCAT_MAX_SLAVES

/* Maximum number of slave-fmmu supported. */
#undef LIBETHERCAT_MAX_SLAVE_FMMU

/* Maximum number of slave-sm supported. */
#undef LIBETHERCAT_MAX_SLAVE_SM

/* Maximum number of string-len supported. */
#undef LIBETHERCAT_MAX_STRING_LEN

/* Enable Mailbox CoE support. */
#undef LIBETHERCAT_MBX_SUPPORT_COE

/* Enable Mailbox EoE support. */
#undef LIBETHERCAT_MBX_SUPPORT_EOE

/* Enable Mailbox FoE support. */
#undef LIBETHERCAT_MBX_SUPPORT_FOE

/* Enable Mailbox SoE support. */
#undef LIBETHERCAT_MBX_SUPPORT_SOE

/* Build with bpf hw device layer. */
#undef LIBETHERCAT_BUILD_DEVICE_BPF

/* Build with file hw device layer. */
#undef LIBETHERCAT_BUILD_DEVICE_FILE

/* Build with pikeos hw device layer. */
#undef LIBETHERCAT_BUILD_DEVICE_PIKEOS

/* Build with sock-raw hw device layer. */
#undef LIBETHERCAT_BUILD_DEVICE_SOCK_RAW_LEGACY

/* Build with sock-raw mmaped hw device layer. */
#undef LIBETHERCAT_BUILD_DEVICE_SOCK_RAW_MMAPED

/* Use PikeOS build */
#undef LIBETHERCAT_BUILD_PIKEOS

/* Use POSIX build on linux */
#undef LIBETHERCAT_BUILD_POSIX

/* Use VxWorks build */
#undef LIBETHERCAT_BUILD_VXWORKS

#endif /* LIBETHERCAT_SETTINGS_H */



